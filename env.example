MSSQL_DRIVER={ODBC Driver 17 for SQL Server}
MSSQL_SERVER=localhost
MSSQL_DATABASE=your_database
MSSQL_USER=your_username
MSSQL_PASSWORD=your_password
MSSQL_PORT=1433
TrustServerCertificate=yes
Trusted_Connection=no

# Microsoft Fabric SQL Database Configuration
# AUTH_METHOD options: sql, azure_ad_interactive, azure_ad_password, azure_ad
AUTH_METHOD=sql

# For Fabric SQL Database (uncomment and configure):
# MSSQL_SERVER=your-workspace.datawarehouse.fabric.microsoft.com
# MSSQL_DATABASE=your_fabric_database
# MSSQL_USER=your_email@domain.com
# AUTH_METHOD=azure_ad_interactive

# For Azure AD Service Principal authentication:
# AZURE_TENANT_ID=your_tenant_id
# AZURE_CLIENT_ID=your_client_id
# AZURE_CLIENT_SECRET=your_client_secret
# AUTH_METHOD=azure_ad 